func div::(a::int, b::int)->int
begin
    var c::int;
    if a < b then
        c <- 0;
    else
        c <- div(a - b, b) + 1;
    end
    return c;
end

func mod::(a::int, b::int)->int
begin
    var c::int;
    if a < b then
        c <- a;
    else
        c <- mod(a - b, b);
    end
    return c;
end

func main::()->int
begin
    var c::int;

    print div( 123 , 5 );
    print mod( 123 , 5 );

    return 0;
end
