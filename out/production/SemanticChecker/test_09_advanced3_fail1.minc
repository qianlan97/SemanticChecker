func Fib::(n::int)->int
begin
    if n < 0
    then
        return 0;
    else
        if n = 0
        then
            return 0;
        else
            return Fib(n-1 + Fib(n-2));
        end
    end
end

func main::(n::bool)->int
begin
    var fib::int ;

    fib <- Fib(n);
    print fib;

    return 0;
end
