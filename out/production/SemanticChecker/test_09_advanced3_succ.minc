func Fib::(n::int)->int
begin
    if n <= 1 then
        return n;
    else
        return Fib(n-1) + Fib(n-2);
    end
end

func main::()->int
begin
    var fib::int ;

    fib <- Fib(10);
    print fib;

    if fib < 10 then begin
       fib <- 1;
    end else begin
       fib <- 0;
    end end

    return fib;
end
